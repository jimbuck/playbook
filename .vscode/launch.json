{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Debug",
      "program": "${workspaceFolder}/dist/debug.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Bin",
      "program": "${workspaceFolder}/dist/bin/pb.js",
      "cwd": "${workspaceFolder}/test",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}"
    }
  ]
}